#include<stdio.h>
#include<conio.h>
int main()
{
    int a, b, c, d, e, f, g, ch;
    printf("enter two variables a and b here -->");
    scanf("%d %d",&a, &b);
    printf("enter operation \n 1. Additon \n 2. Difference \n 3. Multiplicaton 4. division ");
    scanf("%d", &ch);
    switch(ch)
    {
        case 1:
        c=a+b;
        printf("sum of two numbers is %d.", c);
        break;
    
        case 2:
        d=a-b;
        printf("Difference of two numbers is %d.", e);
        break;
        case 3:
        e=a*b;
        printf("Product of two numbers is %d.", e);
        break;
        case 4:
        f=a/b;
        printf("a/b = %d.", f);
        break;
        case 5:
        g=a%b;
        printf("Mod of a and b is %d.", g);
        break;
        default:
        printf("Error!! Enter correct choice.");
    
     }
}
