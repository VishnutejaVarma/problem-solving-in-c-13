#include <stdio.h>

int main()
{
    int a,b,c;
    printf("the value of 3 sides of triangle");
    scanf("%d %d %d",&a,&b,&c);
    if(a==b && b==c && c==a)
    {
        printf("the triangle  is equilateral triangle");
    }
    else if(a==b || b==c || c==a)
    {
        printf("the tringle is isoscles triangle");
    }
else
{
    printf("the traingle is scalen");
}
    return 0;
}
